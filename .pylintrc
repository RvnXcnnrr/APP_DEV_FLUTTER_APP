[MASTER]
disable=
    C0111, # missing-docstring
    C0103, # invalid-name
    C0303, # trailing-whitespace
    C0330, # bad-continuation
    C1801, # len-as-condition
    W0511, # fixme
    W0212, # protected-access
    W0703, # broad-except
    W0621, # redefined-outer-name
    W0622, # redefined-builtin
    W0702, # bare-except
    W0613, # unused-argument
    W0603, # global-statement
    W0614, # unused-wildcard-import
    W0401, # wildcard-import
    W0102, # dangerous-default-value
    W0221, # arguments-differ
    W0201, # attribute-defined-outside-init
    W0223, # abstract-method
    W0107, # unnecessary-pass
    R0201, # no-self-use
    R0903, # too-few-public-methods
    R0913, # too-many-arguments
    R0914, # too-many-locals
    R0915, # too-many-statements
    R0902, # too-many-instance-attributes
    R0904, # too-many-public-methods
    R0912, # too-many-branches
    R0801, # duplicate-code
    E1101, # no-member
    E0611, # no-name-in-module
    E0401, # import-error
    E1136, # unsubscriptable-object
    E1135, # unsupported-membership-test
    E1102, # not-callable

[FORMAT]
max-line-length=120

[TYPECHECK]
ignored-modules=django.urls.conf

[MESSAGES CONTROL]
disable=all
enable=F,E,unreachable,duplicate-key,unnecessary-semicolon,global-variable-not-assigned,unused-variable,unused-wildcard-import,binary-op-exception,bad-format-string,anomalous-backslash-in-string,bad-open-mode
